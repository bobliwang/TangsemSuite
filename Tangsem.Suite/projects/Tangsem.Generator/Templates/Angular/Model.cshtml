@* Generator: Template *@


@using System.Linq
@inherits Tangsem.Generator.Templates.SingleTableMetadataTemplate



export interface @(TableMetadata.EntityName) {


@foreach (var col in @TableMetadata.Columns.Where(c => c.ExtraColumnMeta == null || string.IsNullOrWhiteSpace(c.ExtraColumnMeta.JsonType)))
{<text>

        @col.PropertyName: @col.CSharpTypeAsString
</text>}

@foreach (var col in @TableMetadata.Columns.Where(c => c.ExtraColumnMeta != null && !string.IsNullOrWhiteSpace(c.ExtraColumnMeta.JsonType)))
{<text>
        @col.PropertyName: @col.ExtraColumnMeta.JsonType
</text>}

}